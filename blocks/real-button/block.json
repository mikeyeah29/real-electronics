{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "real-electronics/real-button",
    "title": "Real Button",
    "category": "design",
    "icon": "button",
    "description": "Real Electronics button with variants and optional SVG icon.",
    "supports": {
        "html": false
    },
    "attributes": {
        "label": {
            "type": "string",
            "default": "Button"
        },
        "url": {
            "type": "string",
            "default": ""
        },
        "type": {
            "type": "string",
            "default": "primary"
        },
        "icon": {
            "type": "string",
            "default": ""
        }
    },
    "editorScript": "file:./index.js",
    "style": "file:./style.css",
    "editorStyle": "file:./editor.css",
    "render": "file:./render.php"
}